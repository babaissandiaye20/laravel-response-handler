{
    "name": "babaissandiaye/laravel-response-handler",
    "description": "Package Laravel pour la gestion standardisée des réponses JSON et exceptions",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "response",
        "exception",
        "json",
        "api"
    ],
    "authors": [
        {
            "name": "Baba Issa Ndiaye",
            "email": "babaissandiaye@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "BabaissaNdiaye\\LaravelResponseHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BabaissaNdiaye\\LaravelResponseHandler\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BabaissaNdiaye\\LaravelResponseHandler\\ResponseHandlerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}